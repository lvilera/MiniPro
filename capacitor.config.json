{
  "appId": "com.sportscard.album",
  "appName": "Sports Card Album",
  "webDir": ".",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#121018",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#121018"
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark"
    }
  },
  "ios": {
    "contentInset": "automatic",
    "scheme": "sportscard"
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#121018"
  }
}
